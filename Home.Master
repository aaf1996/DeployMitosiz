        if (screen.width > 1279 & 1366 > screen.width) {
            var controls = document.querySelectorAll('.menu__item');
            // también puedes usar forEach aquí
            [].forEach.call(controls, el => {
                el.addEventListener('click', btnClick, false)
            })
            function btnClick() {
                // use la función Array para este léxico
                [].forEach.call(controls, el => {
                    // excepto por el elemento en el que se hizo clic, eliminar la clase activarDesplegableMenu
                    if (el !== this) el.classList.remove('activarDesplegableMenu');
                });
                // Alternar la clase en el botón al hacer clic.
                this.classList.toggle('activarDesplegableMenu');
            }
        }

